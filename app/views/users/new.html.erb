<div class="forms">
  <h1>Sign Up</h1>
  <h3>Tip: for access premium user create an account with:<h3>
  <p>username: test@test.com</p>
  <p>password: password</p>
  <%= form_for @user do |f|%>
    <%= f.label :username%><br>
    <%= f.text_field :username%><br>
    <%= f.label :password%><br>
    <%= f.password_field :password%><br>
    <%= f.submit %>
  <%end%>
</div>